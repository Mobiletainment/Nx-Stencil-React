{
  "version": 1,
  "projects": {
    "frontend1": {
      "root": "apps/frontend1",
      "sourceRoot": "apps/frontend1/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "withDeps": true,
            "outputPath": "dist/apps/frontend1",
            "index": "apps/frontend1/src/index.html",
            "main": "apps/frontend1/src/main.tsx",
            "polyfills": "apps/frontend1/src/polyfills.ts",
            "tsConfig": "apps/frontend1/tsconfig.app.json",
            "assets": [
              "apps/frontend1/src/favicon.ico",
              "apps/frontend1/src/assets"
            ],
            "styles": [
              "apps/frontend1/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/frontend1/src/environments/environment.ts",
                  "with": "apps/frontend1/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          },
          "outputs": [
            "{options.outputPath}"
          ]
        },
        "serve": {
          "builder": "@nrwl/web:file-server",
          "options": {
            "buildTarget": "frontend1:build",
            "withDeps": true,
            "parallel": true
          },
          "configurations": {
            "production": {
              "buildTarget": "frontend1:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend1/**/*.js",
              "apps/frontend1/**/*.jsx",
              "apps/frontend1/**/*.ts",
              "apps/frontend1/**/*.tsx",
              "apps/frontend1/**/*.spec.ts",
              "apps/frontend1/**/*.spec.tsx",
              "apps/frontend1/**/*.spec.js",
              "apps/frontend1/**/*.spec.jsx",
              "apps/frontend1/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/frontend1/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": [
            "coverage/apps/frontend1"
          ]
        }
      }
    },
    "frontend1-e2e": {
      "root": "apps/frontend1-e2e",
      "sourceRoot": "apps/frontend1-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/frontend1-e2e/cypress.json",
            "tsConfig": "apps/frontend1-e2e/tsconfig.e2e.json",
            "devServerTarget": "frontend1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "frontend1:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/frontend1-e2e/src/**/*.ts",
              "apps/frontend1-e2e/src/**/*.js"
            ]
          }
        }
      }
    },
    "ui-components": {
      "projectType": "library",
      "root": "libs/ui-components",
      "sourceRoot": "libs/ui-components/src",
      "architect": {
        "build": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/ui-components/stencil.config.ts"
          }
        },
        "test": {
          "builder": "@nxext/stencil:test",
          "options": {
            "projectType": "library",
            "configPath": "libs/ui-components/stencil.config.ts"
          }
        },
        "e2e": {
          "builder": "@nxext/stencil:e2e",
          "options": {
            "projectType": "library",
            "configPath": "libs/ui-components/stencil.config.ts"
          }
        },
        "serve": {
          "builder": "@nxext/stencil:build",
          "options": {
            "projectType": "library",
            "configPath": "libs/ui-components/stencil.config.ts",
            "watch": true,
            "serve": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "scss"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "frontend1"
}